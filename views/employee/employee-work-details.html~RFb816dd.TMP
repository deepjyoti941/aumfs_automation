<div class="page" ng-controller="EmployeeWorkDetailsCtrl">
	<ol class="breadcrumb-alt">
		<li><a href="#/dashboard">Dashboard</a></li>
		<li><a href="#/employee-listing">Employee List</a></li>
		<li><a href="#/employee-status">Employee Status</a></li>
		<li class="active"><a href="">Employee Work Details </a></li>
	</ol>
	<section class="panel panel-default">
		<div class="panel-heading"><strong><span class="glyphicon glyphicon-th"></span>Employee Work Details</strong></div>
		<div class="panel-body">
				<table class="table table-bordered table-striped table-responsive">
					<thead>
						<tr>
							<th>
								<div class="th">
									ID
								</div>
							</th>
							<th>
								<div class="th">
									Assigned Date
								</div>
							</th>
							<th>
								<div class="th">
									Assigned Services
								</div>
							</th>
						</tr>
					</thead>
					<tbody>
						<tr ng:repeat="item in employee_services">
            				<td>{{item.id}}</td>
            				<td>{{item.assigned_date}}</td>
            				<td>{{item.assigned_service}}</td>
            				<td><a href ng:click="removeItem($index)" class="btn-icon btn-icon-sm btn-danger"><i class="fa fa-times"></i></a></td>
						</tr>
					</tbody>
				</table>	    	
	    </div>
	</section>
</div>