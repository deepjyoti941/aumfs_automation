                    <div class="col-md-12" ng-show="filteredItems == 0">
                        <div class="col-md-12">
                            <h4>No customers found</h4>
                        </div>
                    </div>
                    <div class="col-md-12" ng-show="filteredItems > 0">    
                        <div pagination="" page="currentPage" on-select-page="setPage(page)" boundary-links="true" total-items="filteredItems" items-per-page="entryLimit" class="pagination-small" previous-text="&laquo;" next-text="&raquo;"></div>     
                    </div>
                    </div>

                </tab>
            </tabset>
        </div>
        </div>

    </div>
    <div class="panel panel-default">
        <div class="panel-heading"><strong><span class="glyphicon glyphicon-th"></span> AUM Notifications</strong>
           <tabset justified="true" class="ui-tab">
                <tab heading="Admin Notifications">Admin Notifications</tab>
                <tab heading="Employee Notifications">Employee Notifications</tab>
            </tabset>
        </div>

    </div>
    <div class="panel panel-default">
        <div class="panel-heading"><strong><span class="glyphicon glyphicon-th"></span> AMC Notifications</strong>
           <tabset justified="true" class="ui-tab">
                <tab heading="Admin Notifications">Admin Notifications</tab>
                <tab heading="Employee Notifications">Employee Notifications</tab>
            </tabset>
        </div>

    </div>

</div>
  